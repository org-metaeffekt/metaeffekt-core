#!/usr/bin/env groovy
pipeline {
    agent any
    tools {
        maven 'maven-3.9.6'
        jdk 'ZULU17'
    }
    stages {
        stage('Build and Test') {
            steps {
                withMaven(maven: 'maven-3.9.12') {
                    sh 'mvn -B clean install'
                }
            }
        }
    }
}